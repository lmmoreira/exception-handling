<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/tx
                           http://www.springframework.org/schema/tx/spring-tx-2.5.xsd"
        default-autowire="byName">

	<bean id="illegalArgumentException" class="java.lang.IllegalArgumentException"/>
	<bean id="indexOutOfBoundsException" class="java.lang.IndexOutOfBoundsException"/>
	<bean id="arithmeticException" class="java.lang.ArithmeticException"/>
	<bean id="arrayStoreException" class="java.lang.ArrayStoreException"/>
	<bean id="excecaoAritimetica" class="br.exceptionhandlingweb.except.ExcecaoAritimetica"/>
	<bean id="excecaoArmazenamentoEmArray" class="br.exceptionhandlingweb.except.ExcecaoArmazenamentoEmArray"/>
	<bean id="myHandler" class="br.exceptionhandlingweb.handler.MyHandler"/>    
</beans>